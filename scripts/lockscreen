#!/bin/bash
BG=2B303B
TEXT=FFFFFF00
INSIDE_CIR=313D46FF
INSIDE_CIR_VER=0000001C
OUTSIDE_CIR=4F5B66FF
LINE_SEP=FFFFFF00
KEYPRESS_HL=C0C5CEFF
OUTER_RING_VERIFY=00000000
OUTER_RING_WRONG=BF616AFF
